@model List<gpw.Models.cat>

@if (Model != null)
{
    foreach (var item in @Model)
    {
        <!--Our Agent Warp Start-->
        <section class="kf_our_agent_bg" style="padding: 20px 10px;">
            <!--Heading Wrap Start-->
            <div class="kf_heading_1">
                <h3><a href="/News/tin/@item.cat_url">@item.cat_name</a></h3>
                <span class="kf_property_line"></span>
            </div>
            <!--Heading Wrap End-->
            <!--Agent List Wrap Start-->
            <div class="NewsLoad">
                <div>
                    <div class="row">
                        @{Html.RenderAction("LoadNewInCat", new { cat_id = @item.id });}
                    </div>
                </div>
            </div>
            <!--Agent List Wrap End-->
        </section>
        <!--Our Agent Warp End-->
    }
}

