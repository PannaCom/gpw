@model List<gpw.Models.quan_he_thanh_vien>

@if (Model != null)
{
    if (Model.Count > 0)
    {
        <h3>Danh sách thành viên: </h3>
        <ul>
            @foreach (var item in Model)
            {
                <li>
                    @item.ten_quan_he - <a href="/ThanhVien/Details/@item.thanh_vien_qh_id">@item.ten_thanh_vien</a> - quê quán: @item.que_quan<span>.</span>
                    @{Html.RenderAction("LoadThanhVienCungQH", "ThanhVien", new { tenthanhvien = @item.ten_thanh_vien });}
                </li>
            }
        </ul>
    }

}
